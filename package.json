    "start:ts": "ts-node src/index.ts",
    "build": "tsc src/index.ts --outDir ./dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@types/node": "^18.7.9",
    "eslint": "^8.22.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
