{
  "name": "ts-node-starter-template",
  "version": "1.0.0",
  "description": "",
  "author": "Liu Mingxun <liumingxun@yeah.net>",
  "license": "ISC",
  "keywords": [
    "typescript",
    "template"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typeCheck": "tsc --noEmit",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
